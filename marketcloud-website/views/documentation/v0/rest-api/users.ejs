        next : 'http://api.marketcloud.it/v0/users/3'
    }

  },
    {
      data : {
        id:3,
        email: "jon@snow.wf"
        name: "Jon Snow",
        password: "winteriscoming",
        img_url: "http://media.marketcloud.it/gsh18xb1k715A217yC",
      },
      links : {
        first : 'http://api.marketcloud.it/v0/users/1',
        last : 'http://api.marketcloud.it/v0/users/3',
        self : 'http://api.marketcloud.it/v0/users/3',
