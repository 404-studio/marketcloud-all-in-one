<!-- STORE CREATE -->
					<div class="documentation">
						<h3>POST /stores</h3>
						<p class="lead">Creates a new Store.</p>
						<pre class="code http-call">POST http://api.marketcloud.it/v0/stores</pre>
						<h5>Parameters</h5>
						<table class="table table-striped documentation">
							<thead><tr><th>Field</th><th>Type</th><th>Description</th></tr></thead>
							<tbody>
								<tr>
									<td>name <span class="label label-success">Required</span></td>
									<td>String</td>
									<td>The store name</td>
								</tr>
								<tr>
									<td>owner_email <span class="label label-success">Required</span></td>
									<td>String</td>
									<td>The email of the store owner.</td>
								</tr>
								<tr>
									<td>description</td>
									<td>String</td>
									<td>A description of the store.</td>
								</tr>
							</tbody>
						</table>
						<h5>Example Request</h5>
						<iframe src="//api.apiembed.com/?source=https://raw.githubusercontent.com/Marketcloud/json-request-examples/master/carts/update.json&targets=shell:curl,java:unirest,python:requests,ruby:native,objc:nsurlsession,go:native,csharp:restsharp" frameborder="0" scrolling="no" width="100%" height="300px" seamless></iframe>
						<h5>Example JSON response (200)</h5>
						<pre class="code prettyprint" >
{
	status:true,
	data: {
		id:3,
		name: "The Beer Shop",
		owner_email: "thebeer@shop.com",
		description: "The Beer Shop is the coolest shop ever. Come every week to taste our always new beer!"
		}
	links : {
		self : "http://api.marketcloud.it/v0/stores/3"
	}
}</pre>								
							</div>
<!-- / STORE CREATE -->						

<!-- STORE LIST -->
						<div class="documentation">
						<h3>GET /stores</h3>
						<p class="lead">Retrieves a paginated response of a list of stores from your application.</p>
						<pre class="code http-call">GET http://api.marketcloud.it/v0/stores</pre>
						<h5>Request parameters</h5>
						<table class="table table-striped documentation">
									<thead>
										<tr>
										<th>Field</th>
										<th>Type</th>
										<th>Description</th>
										</tr>
									</thead>
									<tbody>
								<tr>
									<td class="code">fields</td>
									<td>String</td>
									<td>Comma separated list of attribute names to retrieve. Use it to retrieve only the fields you need.</td>
								</tr>
								<tr>
									<td  class="code">skip</td>
									<td>Number</td>
									<td>How many resources to be skipped</td>
								</tr>
								<tr>
									<td class="code">limit</td>
									<td>Number</td>
									<td>How many resource to be fetched</td>
								</tr>
							</tbody>
								</table>
						<h5>Example Request</h5>
						<iframe src="//api.apiembed.com/?source=https://raw.githubusercontent.com/Marketcloud/json-request-examples/master/carts/update.json&targets=shell:curl,java:unirest,python:requests,ruby:native,objc:nsurlsession,go:native,csharp:restsharp" frameborder="0" scrolling="no" width="100%" height="300px" seamless></iframe>
						<h5>Example JSON response (200)</h5>
						<pre class="code prettyprint" >
{
	status:true,
	data: [ {
		data : {
			id:1,
			name: "The Rocknrolla",
			owner_email: "rocknrolla@rock.com",
			description: "Find the best rock shirt ever. Only @Rocknrolla!"
		},
		links : {
				first : 'http://api.marketcloud.it/v0/stores/1',
