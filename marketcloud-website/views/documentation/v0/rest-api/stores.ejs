				self : 'http://api.marketcloud.it/v0/stores/1',
				prev : null,
				next : 'http://api.marketcloud.it/v0/stores/3'
		}

	},{
		data : {
			id:3,
			name: "The Beer Shop",
			owner_email: "thebeer@shop.com",
			description: "The Beer Shop is the coolest shop ever. Come every week to taste our always new beer!"
		},
		links : {
			first : 'http://api.marketcloud.it/v0/stores/1',
			last : 'http://api.marketcloud.it/v0/stores/3',
			self : 'http://api.marketcloud.it/v0/stores/3',
			prev : 'http://api.marketcloud.it/v0/stores/1',
			next : null

		}
	}
	
}</pre>								

