<!-- files CREATE -->
					<div class="documentation">
						<h3>POST /files</h3>
						<p class="lead">Uploads a Base64 encoded file.</p>
						<pre><code class="http">POST http://api.marketcloud.it/v0/files</code></pre>
						<h5>Parameters</h5>
						<div class="table-responsive"><table class="table table-striped documentation">
							<thead>
								<tr>
									<th>Attribute</th>
									<th>Type</th>
									<th>Description</th>
								</tr>
							</thead>
							<tbody>
								<tr>
									<td class="code">name <br/><span class="label label-success">Required</span></td>
									<td>String</td>
									<td>Assign a name to the file to make it recognizable. Can be used as alternate name in hypermedia.</td>
								</tr>
								<tr>
									<td class="code">filename <br/><span class="label label-success">Required</span></td>
									<td>String</td>
									<td>The name to give to the file</td>
								</tr>
								<tr>
									<td class="code">file <br/><span class="label label-success">Required</span></td>
									<td>String</td>
									<td>Base64 encoded string representing the file.</td>
								</tr>
								<tr>
									<td class="code">description</td>
									<td>String</td>
									<td>A description of the file</td>
								</tr>
								<tr>
									<td class="code">slug</td>
									<td>String</td>
									<td>A URL friendly string</td>
								</tr>
							</tbody>
						</table></div>
						<h5>Example Request</h5>
						<iframe src="//api.apiembed.com/?source=https://raw.githubusercontent.com/Marketcloud/json-request-examples/master/files/create.json&targets=shell:curl,java:unirest,python:requests,ruby:native,objc:nsurlsession,go:native,csharp:restsharp" frameborder="0" scrolling="no" width="100%" height="300px" seamless></iframe>
						<h5>Example JSON response (200)</h5>
						<pre><code class="javascript">{
  "status": true,
  "data": {
	"filename": "slayer_tshirt.jpg",
	"description": "Classic Slayer tshirt",
	"name": "Slayer Tshirt",
	"mime_type": "image/jpeg",
	"url": "https://marketclouduploads.s3-eu-west-1...",
	"size": 227544,
	"created_at": "2016-09-12T09:07:58.751Z",
	"id": 81576
  }
}</code></pre>							
							</div>
<!-- / file CREATE -->						

