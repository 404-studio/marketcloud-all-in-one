<style type="text/css">
  .fixed-subnavbar {
    position: fixed;
    top: 52px;
    left:0px;
    right: 0px;
    z-index: 10;
  }
</style>
<script type="text/javascript">
  $(document).ready(function(){
    $(document).on('scroll',function(){
      var scroll = $(window).scrollTop();
      if (scroll >= 60) {
        $(".navbar-inverse.subnavbar").addClass('fixed-subnavbar');
        $(".navbar-fixed-top").removeClass('has-shadow');
        $(".navbar-inverse.subnavbar").addClass('has-shadow');
      } else {
        $(".navbar-inverse.subnavbar.fixed-subnavbar").removeClass('fixed-subnavbar');
        $(".navbar-inverse.subnavbar").removeClass('has-shadow');
      }
    })
  })
</script>