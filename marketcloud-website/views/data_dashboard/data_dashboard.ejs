<% include data_header %>

<script>
	window.token = "<%= token %>";
	window.current_application = <%- JSON.stringify(current_application) %>;
	window.user = <%- JSON.stringify(user) %>;
	window.public_key = "<%= public %>";
</script>

<div class="container-fluid" style="padding-bottom: 50px;">
