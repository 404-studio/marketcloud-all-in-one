		<div class="spinner" style="height: 30px;width:30px;margin-top: 5px;border-width: 3px;"></div>
</div>
	<!--  -->
	<ng-view ng-hide="true === showLoadingAnimation"></ng-view>
</div>


<% include data_footer %>